{
    "primary_key": "order_id",
    "group_reject": true,
    "schema_definitions": {
        "order_id": {
            "rules": {
                "required": true,
                "data_type": "string",
                "format": "^\\d{3}-\\d{4}-\\d{3}-\\d{14}$",
                "permitted_value": {
                    "accepted_value": null,
                    "forbidden_value": ["UNKNOWN", "N/A"]
                }
            }
        },
        "item_id": {
            "rules": {
                "required": true,
                "data_type": "string",
                "format": "^ITEM\\d{4}$"
            }
        },
        "item_name": {
            "rules": {
                "required": true
            }
        },
        "quantity": {
            "rules": {
                "required": true,
                "data_type": "int",
                "limit": {
                    "min": 1,
                    "max": null
                }
            }
        },
        "unit_price": {
            "rules": {
                "required": true,
                "data_type": "float",
                "limit": {
                    "min": 0.0,
                    "max": null
                }
            }
        },
        "order_date": {
            "rules": {
                "required": true,
                "data_type": "date"
            }
        },
        "shipping_method": {
            "rules": {}
        },
        "discount_code": {
            "rules": {}
        }
    }
}  